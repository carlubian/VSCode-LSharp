{
    "name": "vscode-lsharp",
    "displayName": "VSCode-LSharp",
    "description": "Provides support for L# source files within Visual Studio Code",
    "version": "1.0.1",
    "publisher": "carlubian",
    "icon": "images/icon-VSCode-LSharp.png",
    "homepage": "https://github.com/carlubian/VSCode-LSharp",
    "galleryBanner": {
        "color": "#007ACC",
        "theme": "dark"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/carlubian/VSCode-LSharp.git"
    },
    "engines": {
        "vscode": "^1.13.0"
    },
    "categories": [
        "Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "ls",
            "aliases": ["L#", "ls"],
            "extensions": [".ls"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "ls",
            "scopeName": "source.lsharp",
            "path": "./syntaxes/ls.tmLanguage.json"
        }]
    }
}
